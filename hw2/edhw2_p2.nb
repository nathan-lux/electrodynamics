Cell[TextData[{
 "The equation we get constant values of \[Xi] against is ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         FractionBox[
          RowBox[{
            SuperscriptBox[
             StyleBox["x", "TI"], "2"], "+", 
            SuperscriptBox[
             StyleBox["y", "TI"], "2"]}], 
          SuperscriptBox[
           StyleBox["a", "TI"], "2"]], "+", 
         FractionBox[
          SuperscriptBox[
           StyleBox["z", "TI"], "2"], 
          SuperscriptBox[
           StyleBox["b", "TI"], "2"]], "\[LongEqual]", "1"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\frac{x^2+y^2}{a^2}+\\frac{z^2}{b^2}=1", "state" -> "Boxes"|>,
