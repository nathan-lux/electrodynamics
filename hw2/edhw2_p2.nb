  RowBox[{
   RowBox[{"{", 
    RowBox[{"d", "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       SqrtBox["2"], 
       SqrtBox[
        FractionBox[
         RowBox[{
          SuperscriptBox["b", "2"], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["a", "2"], "+", 
             SuperscriptBox["b", "2"]}], ")"}], " ", 
           SuperscriptBox["\[Xi]", "2"]}], "-", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["b", "2"], "+", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{"-", "a"}], "+", "b"}], ")"}], " ", 
              RowBox[{"(", 
               RowBox[{"a", "+", "b"}], ")"}], " ", 
              SuperscriptBox["\[Xi]", "2"]}]}], ")"}], " ", 
