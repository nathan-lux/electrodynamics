0c6eae0e7209"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"FullSimplify", "[", 
  RowBox[{"PowerExpand", "[", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["x", "2"], "+", 
          SuperscriptBox["y", "2"]}], ")"}], 
        SuperscriptBox["a", "2"]], "+", 
       FractionBox[
        SuperscriptBox["z", "2"], 
        SuperscriptBox["b", "2"]]}], "==", "1"}], ",", "\[Xi]"}], "]"}], 
   "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.9166850072724047`*^9, 3.916685053118472*^9}},
 CellLabel->"In[18]:=",ExpressionUUID->"555e0fef-bd3b-4798-a742-aac2459b48f4"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\[Xi]", "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       SqrtBox[
        RowBox[{
         RowBox[{"2", " ", 
          SuperscriptBox["a", "2"]}], "-", 
         SuperscriptBox["d", "2"], "+", 
         RowBox[{
          SuperscriptBox["d", "2"], " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Gamma]"}], "]"}]}]}]], 
       SqrtBox[
        FractionBox[
         RowBox[{
          SuperscriptBox["d", "2"], " ", 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["a", "2"], "+", 
            SuperscriptBox["b", "2"], "+", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"a", "-", "b"}], ")"}], " ", 
             RowBox[{"(", 
              RowBox[{"a", "+", "b"}], ")"}], " ", 
             RowBox[{"Cos", "[", 
              RowBox[{"2", " ", "\[Gamma]"}], "]"}]}]}], ")"}]}], 
         SuperscriptBox["b", "2"]]]]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Xi]", "\[Rule]", 
     FractionBox[
      SqrtBox[
       RowBox[{
        RowBox[{"2", " ", 
         SuperscriptBox["a", "2"]}], "-", 
        SuperscriptBox["d", "2"], "+", 
        RowBox[{
         SuperscriptBox["d", "2"], " ", 
         RowBox[{"Cos", "[", 
          RowBox[{"2", " ", "\[Gamma]"}], "]"}]}]}]], 
      SqrtBox[
       FractionBox[
        RowBox[{
         SuperscriptBox["d", "2"], " ", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["a", "2"], "+", 
           SuperscriptBox["b", "2"], "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"a", "-", "b"}], ")"}], " ", 
            RowBox[{"(", 
             RowBox[{"a", "+", "b"}], ")"}], " ", 
            RowBox[{"Cos", "[", 
             RowBox[{"2", " ", "\[Gamma]"}], "]"}]}]}], ")"}]}], 
        SuperscriptBox["b", "2"]]]]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{{3.916685040070919*^9, 3.916685053655719*^9}},
 CellLabel->"Out[18]=",ExpressionUUID->"d78a0072-b51f-41bd-a042-38d98fd3b9cf"]
}, Open  ]],

