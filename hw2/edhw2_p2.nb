   RowBox[{"{", 
    RowBox[{"d", "\[Rule]", 
     FractionBox[
      SqrtBox["2"], 
      SqrtBox[
       FractionBox[
        RowBox[{
         SuperscriptBox["b", "2"], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["a", "2"], "+", 
            SuperscriptBox["b", "2"]}], ")"}], " ", 
          SuperscriptBox["\[Xi]", "2"]}], "-", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["b", "2"], "+", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "a"}], "+", "b"}], ")"}], " ", 
             RowBox[{"(", 
              RowBox[{"a", "+", "b"}], ")"}], " ", 
             SuperscriptBox["\[Xi]", "2"]}]}], ")"}], " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Gamma]"}], "]"}]}]}], 
        RowBox[{
         SuperscriptBox["a", "2"], " ", 
         SuperscriptBox["b", "2"]}]]]]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{3.91668512803893*^9},
 CellLabel->"Out[19]=",ExpressionUUID->"921b0cd5-7260-475e-ad4a-f18be5d5a239"]
