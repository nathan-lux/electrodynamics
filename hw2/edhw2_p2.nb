      RowBox[{"\[Xi]", "^", "2"}]}], "]"}], " ", 
    RowBox[{"Sin", "[", "\[Gamma]", "]"}], " ", 
    RowBox[{"Sin", "[", "\[Phi]", "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"z", " ", "=", " ", 
   RowBox[{"d", " ", "\[Xi]", " ", 
    RowBox[{"Cos", "[", "\[Gamma]", "]"}]}]}], ";"}]}], "Input",
