Cell[TextData[{
 "                                    \[Xi] = ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         FractionBox[
          SqrtBox[
           StyleBox["a", "TI"]], 
          RowBox[{
            StyleBox["d", "TI"], "sin", 
            RowBox[{"(", "\[Gamma]", ")"}]}]], "-", "1"}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\frac{\\sqrt{a}}{d\\sin(\\gamma)}-1", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0ea87508-0079-4b48-a53c-557891309271"],
 " and for d we find ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["d", "TI"], "\[LongEqual]", 
         FractionBox[
          SqrtBox[
           StyleBox["a", "TI"]], 
          RowBox[{
            RowBox[{"(", 
              RowBox[{"1", "+", 
                SuperscriptBox["\[Xi]", "2"]}], ")"}], "sin", 
            RowBox[{"(", "\[Gamma]", ")"}]}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "d=\\frac{\\sqrt{a}}{(1+\\xi^2)\\sin(\\gamma)}", "state" -> "Boxes"|>,
