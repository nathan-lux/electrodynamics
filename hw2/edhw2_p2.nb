Cell[CellGroupData[{
Cell["Homework 2 Problem 2", "Title",
 CellChangeTimes->{{3.916349893008947*^9, 
  3.9163498985714808`*^9}},ExpressionUUID->"62f2cf2a-aa1f-40d1-801f-\
9e597906d99d"],

Cell["\<\
In this problem we are asked to find values corresponding to constant \[Xi], \
\[Gamma], or \[Phi] along with the value of d and either prolate of \
spheroidal coordinates that correspond to the following.\
\>", "Text",
 CellChangeTimes->{{3.916349925223044*^9, 3.916349929870962*^9}, {
  3.916350007332415*^9, 
  3.916350074199833*^9}},ExpressionUUID->"2ef57188-3367-426b-aca8-\
e74c753ee164"],

Cell[CellGroupData[{

Cell["Part a", "Section",
 CellChangeTimes->{{3.916350080117427*^9, 
  3.916350081275742*^9}},ExpressionUUID->"c6839bb9-65d4-4b3a-8c5a-\
f3939801aeca"],

Cell[BoxData[
 RowBox[{
  RowBox[{"$Assumptions", "=", 
   RowBox[{
    RowBox[{"\[Xi]", ">", "0"}], "&&", 
    RowBox[{"\[Gamma]", ">", "0"}], "&&", 
    RowBox[{"\[Phi]", ">", "0"}], "&&", 
    RowBox[{"a", ">", "0"}], "&&", 
    RowBox[{"b", ">", "0"}], "&&", 
    RowBox[{"d", ">", "0"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.916350733074308*^9, 3.916350792376575*^9}, {
  3.916355390628409*^9, 3.916355394280612*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"f254e98f-dddd-4611-ae00-0fa5c023b804"],

Cell[TextData[{
 "Here we find constant coordinate values for a prolate spheroid defined by \
the equation\n\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         FractionBox[
          RowBox[{
            SuperscriptBox[
             StyleBox["x", "TI"], "2"], "+", 
            SuperscriptBox[
             StyleBox["y", "TI"], "2"]}], 
          SuperscriptBox[
           StyleBox["b", "TI"], "2"]], "+", 
         FractionBox[
          SuperscriptBox[
           StyleBox["z", "TI"], "2"], 
          SuperscriptBox[
           StyleBox["a", "TI"], "2"]], "\[LongEqual]", "1"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\frac{x^2+y^2}{b^2}+\\frac{z^2}{a^2}=1", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3dcc59d5-7617-4cbb-bc98-4a4b4a3ccbd5"],
 "\nwhere a > b."
}], "Text",
 CellChangeTimes->{{3.916350087135653*^9, 3.916350116582016*^9}, {
  3.91635014868818*^9, 
  3.916350161227976*^9}},ExpressionUUID->"e5180574-ea66-4a60-be78-\
7e7a420ea8c1"],

Cell["\<\
We know that for the prolate spheroid, we have the following coordinates\
\>", "Text",
 CellChangeTimes->{{3.916350164535798*^9, 
  3.916350177300569*^9}},ExpressionUUID->"5fa1a5e3-5e21-4474-9ef5-\
e3f30dd42297"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"x", "=", 
   RowBox[{"d", " ", "\[Xi]", " ", 
    RowBox[{"Sin", "[", "\[Gamma]", "]"}], 
    RowBox[{"Cos", "[", "\[Phi]", "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"y", "=", 
   RowBox[{"d", " ", "\[Xi]", " ", 
    RowBox[{"Sin", "[", "\[Gamma]", "]"}], 
    RowBox[{"Sin", "[", "\[Phi]", "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"z", "=", 
   RowBox[{"d", " ", 
    RowBox[{"Sqrt", "[", 
     RowBox[{"1", "+", 
      RowBox[{"\[Xi]", "^", "2"}]}], "]"}], 
    RowBox[{"Cos", "[", "\[Gamma]", "]"}]}]}], ";"}]}], "Input",
 CellLabel->"In[1]:=",ExpressionUUID->"6571a1a8-f022-4293-961c-3460650243d0"],

Cell["\<\
So we should be able to now just solve for each coordinate of constant value.\
\
\>", "Text",
 CellChangeTimes->{{3.9163502136833572`*^9, 
  3.916350231336171*^9}},ExpressionUUID->"41d760ca-631c-4f77-b17d-\
2ba1945666dd"],

Cell[BoxData[
 RowBox[{
  RowBox[{"eqn", "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x", "2"], "+", 
        SuperscriptBox["y", "2"]}], ")"}], 
      SuperscriptBox["b", "2"]], "+", 
     FractionBox[
      SuperscriptBox["z", "2"], 
      SuperscriptBox["a", "2"]]}], "==", "1"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.916350233857193*^9, 3.91635040035603*^9}, {
  3.9163504381626883`*^9, 3.916350479224443*^9}, {3.916350587242919*^9, 
  3.916350640023909*^9}, {3.916350673753509*^9, 3.916350715656749*^9}},
 CellLabel->"In[5]:=",ExpressionUUID->"378d671f-1bdf-476a-b273-991c95478c2a"],

Cell["Okay so now we can try and solve for \[Xi]", "Text",
 CellChangeTimes->{{3.916682026016591*^9, 
  3.916682054124292*^9}},ExpressionUUID->"7de1f639-d2e1-4fce-962e-\
f1ed2aea1ef7"],

Cell[CellGroupData[{

Cell[BoxData["eqn"], "Input",
 CellChangeTimes->{{3.916682228460894*^9, 3.916682235059168*^9}, {
  3.9166822919205933`*^9, 3.916682303839072*^9}, {3.916682334021791*^9, 
  3.916682335496805*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"b6792092-ae32-4f9a-b6d7-cd96f690c89d"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   FractionBox[
    RowBox[{
     SuperscriptBox["d", "2"], " ", 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       SuperscriptBox["\[Xi]", "2"]}], ")"}], " ", 
     SuperscriptBox[
      RowBox[{"Cos", "[", "\[Gamma]", "]"}], "2"]}], 
    SuperscriptBox["a", "2"]], "+", 
   FractionBox[
    RowBox[{
     RowBox[{
      SuperscriptBox["d", "2"], " ", 
      SuperscriptBox["\[Xi]", "2"], " ", 
      SuperscriptBox[
       RowBox[{"Cos", "[", "\[Phi]", "]"}], "2"], " ", 
      SuperscriptBox[
       RowBox[{"Sin", "[", "\[Gamma]", "]"}], "2"]}], "+", 
     RowBox[{
      SuperscriptBox["d", "2"], " ", 
      SuperscriptBox["\[Xi]", "2"], " ", 
      SuperscriptBox[
       RowBox[{"Sin", "[", "\[Gamma]", "]"}], "2"], " ", 
      SuperscriptBox[
       RowBox[{"Sin", "[", "\[Phi]", "]"}], "2"]}]}], 
    SuperscriptBox["b", "2"]]}], "\[Equal]", "1"}]], "Output",
 CellChangeTimes->{
  3.9166822381158237`*^9, {3.916682297818375*^9, 3.91668230596903*^9}, 
   3.916682335973297*^9},
 CellLabel->"Out[9]=",ExpressionUUID->"4de38656-792b-48bf-b28d-4cb03129cddb"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"FullSimplify", "[", 
  RowBox[{"PowerExpand", "[", 
   RowBox[{"Solve", "[", 
    RowBox[{"eqn", ",", "\[Xi]"}], "]"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.916682348195218*^9, 3.91668235200174*^9}, {
  3.9166829020977983`*^9, 3.91668291087506*^9}},
 CellLabel->"In[19]:=",ExpressionUUID->"cb3349af-7782-4f51-b9db-fa89cc58f3d3"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"\[Xi]", "\[Rule]", 
    TemplateBox[{
      FractionBox[
       RowBox[{"b", " ", 
         SqrtBox[
          RowBox[{
            RowBox[{
              RowBox[{"-", "2"}], " ", 
              SuperscriptBox["a", "2"]}], "+", 
            SuperscriptBox["d", "2"], "+", 
            RowBox[{
              SuperscriptBox["d", "2"], " ", 
              RowBox[{"Cos", "[", 
                RowBox[{"2", " ", "\[Gamma]"}], "]"}]}]}]]}], 
       RowBox[{"d", " ", 
         SqrtBox[
          RowBox[{
            RowBox[{"-", 
              SuperscriptBox["a", "2"]}], "-", 
            SuperscriptBox["b", "2"], "+", 
            RowBox[{
              RowBox[{"(", 
                RowBox[{"a", "-", "b"}], ")"}], " ", 
              RowBox[{"(", 
                RowBox[{"a", "+", "b"}], ")"}], " ", 
              RowBox[{"Cos", "[", 
                RowBox[{"2", " ", "\[Gamma]"}], "]"}]}]}]]}]], 
      InterpretationBox[
       DynamicModuleBox[{Typeset`open = False}, 
        TemplateBox[{"Expression", 
          StyleBox[
           TagBox[
            TooltipBox["\"condition\"", 
             TagBox[
              RowBox[{
                RowBox[{"(", 
                  RowBox[{
                    RowBox[{"d", ">", "a"}], "&&", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{
                    TemplateBox[{"1"}, "C"], "\[Element]", 
                    TemplateBox[{}, "Integers"]}], "&&", 
                    RowBox[{
                    TemplateBox[{"1"}, "C"], "\[GreaterEqual]", "1"}], "&&", 
                    RowBox[{
                    RowBox[{
                    RowBox[{
                    FractionBox["1", "2"], " ", 
                    RowBox[{"ArcCos", "[", 
                    RowBox[{
                    RowBox[{"-", "1"}], "+", 
                    FractionBox[
                    RowBox[{"2", " ", 
                    SuperscriptBox["a", "2"]}], 
                    SuperscriptBox["d", "2"]]}], "]"}]}], "+", 
                    RowBox[{"\[Pi]", " ", 
                    TemplateBox[{"1"}, "C"]}]}], "<", "\[Gamma]", "<", 
                    RowBox[{"\[Pi]", "-", 
                    RowBox[{
                    FractionBox["1", "2"], " ", 
                    RowBox[{"ArcCos", "[", 
                    RowBox[{
                    RowBox[{"-", "1"}], "+", 
                    FractionBox[
                    RowBox[{"2", " ", 
                    SuperscriptBox["a", "2"]}], 
                    SuperscriptBox["d", "2"]]}], "]"}]}], "+", 
                    RowBox[{"\[Pi]", " ", 
                    TemplateBox[{"1"}, "C"]}]}]}]}], ")"}], "||", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"2", " ", "\[Gamma]"}], ">", 
                    RowBox[{"ArcCos", "[", 
                    RowBox[{
                    RowBox[{"-", "1"}], "+", 
                    FractionBox[
                    RowBox[{"2", " ", 
                    SuperscriptBox["a", "2"]}], 
                    SuperscriptBox["d", "2"]]}], "]"}]}], "&&", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"2", " ", "\[Gamma]"}], "+", 
                    RowBox[{"ArcCos", "[", 
                    RowBox[{
                    RowBox[{"-", "1"}], "+", 
                    FractionBox[
                    RowBox[{"2", " ", 
                    SuperscriptBox["a", "2"]}], 
                    SuperscriptBox["d", "2"]]}], "]"}]}], "<", 
                    RowBox[{"2", " ", "\[Pi]"}]}]}], ")"}]}], ")"}]}], ")"}], 
                "||", 
                RowBox[{"d", "<", "a"}]}], Short[#, 7]& ]], Annotation[#, 
             Short[
              Or[
               And[$CellContext`d > $CellContext`a, 
                Or[
                 And[
                  Element[
                   C[1], Integers], C[1] >= 1, 
                  Inequality[
                  Rational[1, 2] 
                    ArcCos[-1 + 2 $CellContext`a^2 $CellContext`d^(-2)] + 
                   Pi C[1], Less, $CellContext`\[Gamma], Less, Pi + 
                   Rational[-1, 2] 
                    ArcCos[-1 + 2 $CellContext`a^2 $CellContext`d^(-2)] + 
                   Pi C[1]]], 
                 And[
                 2 $CellContext`\[Gamma] > 
                  ArcCos[-1 + 2 $CellContext`a^2 $CellContext`d^(-2)], 
                  2 $CellContext`\[Gamma] + 
                   ArcCos[-1 + 2 $CellContext`a^2 $CellContext`d^(-2)] < 
                  2 Pi]]], $CellContext`d < $CellContext`a], 7], 
             "Tooltip"]& ], "IconizedCustomName", StripOnInput -> False], 
          GridBox[{{
             RowBox[{
               TagBox["\"Head: \"", "IconizedLabel"], "\[InvisibleSpace]", 
               TagBox["Or", "IconizedItem"]}]}, {
             RowBox[{
               TagBox["\"Byte count: \"", "IconizedLabel"], 
               "\[InvisibleSpace]", 
               TagBox["3080", "IconizedItem"]}]}}, 
           GridBoxAlignment -> {"Columns" -> {{Left}}}, DefaultBaseStyle -> 
           "Column", 
           GridBoxItemSize -> {
            "Columns" -> {{Automatic}}, "Rows" -> {{Automatic}}}], 
          Dynamic[Typeset`open]}, "IconizedObject"]], 
       Or[
        And[$CellContext`d > $CellContext`a, 
         Or[
          And[
           Element[
            C[1], Integers], C[1] >= 1, 
           Inequality[
           Rational[1, 2] ArcCos[-1 + 2 $CellContext`a^2 $CellContext`d^(-2)] + 
            Pi C[1], Less, $CellContext`\[Gamma], Less, Pi + 
            Rational[-1, 2] 
             ArcCos[-1 + 2 $CellContext`a^2 $CellContext`d^(-2)] + Pi C[1]]], 
          
          And[
          2 $CellContext`\[Gamma] > 
           ArcCos[-1 + 2 $CellContext`a^2 $CellContext`d^(-2)], 
           2 $CellContext`\[Gamma] + 
            ArcCos[-1 + 2 $CellContext`a^2 $CellContext`d^(-2)] < 
           2 Pi]]], $CellContext`d < $CellContext`a], SelectWithContents -> 
       True, Selectable -> False]},
     "ConditionalExpression"]}], "}"}], "}"}]], "Output",
 CellChangeTimes->{3.9166823531555653`*^9, 3.916682912796303*^9},
 CellLabel->"Out[19]=",ExpressionUUID->"c167975f-6c08-4669-800e-6a0651cb1514"]
}, Open  ]],

Cell["And we can also find that the variable d is given as", "Text",
 CellChangeTimes->{{3.916683074943008*^9, 
  3.9166830856024303`*^9}},ExpressionUUID->"91b0d08d-046a-474f-a3ee-\
fd41d013c3fd"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"FullSimplify", "[", 
  RowBox[{"PowerExpand", "[", 
   RowBox[{"Solve", "[", 
    RowBox[{"eqn", ",", "d"}], "]"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.9166830866143293`*^9, 3.916683108707035*^9}},
 CellLabel->"In[21]:=",ExpressionUUID->"7c9ac213-4d39-4cd7-848c-508ff42e474f"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"d", "\[Rule]", 
    FractionBox[
     RowBox[{
      SqrtBox["2"], " ", "a", " ", "b"}], 
     SqrtBox[
      RowBox[{
       SuperscriptBox["b", "2"], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["a", "2"], "+", 
          SuperscriptBox["b", "2"]}], ")"}], " ", 
        SuperscriptBox["\[Xi]", "2"]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["b", "2"], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "a"}], "+", "b"}], ")"}], " ", 
           RowBox[{"(", 
            RowBox[{"a", "+", "b"}], ")"}], " ", 
           SuperscriptBox["\[Xi]", "2"]}]}], ")"}], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "\[Gamma]"}], "]"}]}]}]]]}], "}"}], 
  "}"}]], "Output",
 CellChangeTimes->{3.916683109792224*^9},
 CellLabel->"Out[21]=",ExpressionUUID->"fc62e69a-555a-4914-8c71-61f4af09e563"]
}, Open  ]],

